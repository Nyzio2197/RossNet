  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rh9891/ContactFox/issues"
  },
  "homepage": "https://github.com/rh9891/ContactFox#readme",
  "dependencies": {
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.15",
    "react": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.20"
